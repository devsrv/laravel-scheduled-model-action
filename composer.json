{
    "name": "devsrv/laravel-scheduled-action",
    "description": "Handle Scheduled Model Action For Laravel",
    "keywords": [
        "laravel",
        "devsrv"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sourav Rakshit",
            "email": "hello@itsrav.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "illuminate/config": "^7.0 || ^8.0",
        "illuminate/database": "^7.0 || ^8.0",
        "illuminate/support": "^7.0 || ^8.0",
        "bensampo/laravel-enum": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Devsrv\\ScheduledAction\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Devsrv\\ScheduledAction\\ActionableServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
