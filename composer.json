{
    "name": "devsrv/laravel-scheduled-action",
    "description": "Handle Scheduled Model Action For Laravel",
    "keywords": [
        "laravel",
        "devsrv",
        "schedule",
        "action"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sourav Rakshit",
            "email": "hello@itsrav.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "illuminate/database": "^7.0|^8.0|^9.0|^10.0",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0",
        "nesbot/carbon": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "pestphp/pest": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "Devsrv\\ScheduledAction\\": "src/",
            "Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Devsrv\\ScheduledAction\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Devsrv\\ScheduledAction\\ScheduledActionServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
