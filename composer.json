{
    "name": "devsrv/laravel-scheduled-action",
    "description": "Handle Scheduled Model Action For Laravel",
    "keywords": [
        "laravel",
        "devsrv",
        "schedule",
        "action"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sourav Rakshit",
            "email": "hello@itsrav.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "illuminate/config": "^7.0|^8.0",
        "illuminate/database": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "bensampo/laravel-enum": "^3.4"
    },
    "require-dev": {
        "orchestra/testbench": "^6.23",
        "pestphp/pest": "^1.21"
    },
    "autoload": {
        "psr-4": {
            "Devsrv\\ScheduledAction\\": "src/",
            "Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Devsrv\\ScheduledAction\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Devsrv\\ScheduledAction\\ActionableServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
